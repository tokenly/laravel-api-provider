{
    "name": "tokenly/laravel-api-provider",
    "description": "An API service provider for Laravel",
    "type": "library",
    "license": "AGPL-3.0-only",
    "authors": [
        {
            "name": "Devon Weller",
            "email": "devon@tokenly.com",
            "homepage": "http://tokenly.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ramsey/uuid": "^3.5",
        "tokenly/token-generator": "dev-master",
        "tokenly/laravel-event-log": "dev-master",
        "tokenly/hmac-auth": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~7"
    },
    "autoload": {
        "psr-4": {"Tokenly\\LaravelApiProvider\\": "src"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tokenly\\LaravelApiProvider\\APIServiceProvider"
            ]
        }
    }
}
